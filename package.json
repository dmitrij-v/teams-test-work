{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon node ./bin/www",
    "lint": "eslint ./src/ --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:fix": "yarn lint --fix",
    "lint:watch": "yarn lint --watch",
    "test": "cross-env NODE_ENV=test mocha --timeout 20000 --recursive tests",
    "test:unit": "cross-env NODE_ENV=test mocha tests/unit",
    "test:integration": "cross-env NODE_ENV=test mocha --timeout 20000 tests/integration",
    "test:watch": "cross-env NODE_ENV=test mocha --watch tests/unit"
  },
  "dependencies": {
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "debug": "~2.6.9",
    "express": "^4.17.3",
    "express-validation": "^1.0.2",
    "http-errors": "~1.6.3",
    "http-status": "^1.4.2",
    "jade": "^1.11.0",
    "joi": "^14.3.1",
    "mocha": "^7.1.2",
    "morgan": "~1.9.1",
    "password-hash": "^1.2.2",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.19.0",
    "sinon": "^9.0.2",
    "standard": "^14.3.1",
    "supertest": "^4.0.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "eslint": "^6.4.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "nodemon": "^1.11.0"
  }
}
